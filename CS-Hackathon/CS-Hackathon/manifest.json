{
    "name": "Zen Timer",
    "version": "1.0.0",
    "description": "Zen mode",
    "manifest_version": 3,
    "author": "Megan Choi & Rathna Chinthalapalli",
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "zen.js"
            ]
        }
    ],
    "host_permissions": [
        "https://www.youtube.com/*",
        "https://www.facebook.com/*",
        "https://www.netflix.com/*"
    ]
}